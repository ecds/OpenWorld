{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "build:css": "./node_modules/node-sass/bin/node-sass ./app/styles/app.scss -o ./app/styles --output-style compressed",
    "dev:css": "npm run build:css && ./node_modules/node-sass/bin/node-sass -rw ./styles -o ./app/styles",
    "dev": "npm run build:css && remix dev",
    "dev:arc": "node ./dev sandbox",
    "start": "remix-serve build",
    "typecheck": "tsc"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/app/index.js",
    "/public/build"
  ],
  "dependencies": {
    "@architect/architect": "^10.10.3",
    "@architect/functions": "^5.4.0",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@remix-run/architect": "^1.14.3",
    "@remix-run/node": "^1.14.3",
    "@remix-run/react": "^1.14.3",
    "@remix-run/serve": "^1.14.3",
    "bootstrap": "^5.2.3",
    "chroma-js": "^2.4.2",
    "isbot": "^3.6.5",
    "maplibre-gl": "^2.4.0",
    "node-sass": "^8.0.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "react-router-bootstrap": "^0.26.2",
    "remix-utils": "^6.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.14.3",
    "@remix-run/eslint-config": "^1.14.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
